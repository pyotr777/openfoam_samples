FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    vessel
    {
        type            kqRWallFunction;
        value           nonuniform 
0
;
    }
    gasInlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 
0
;
        value           nonuniform 
0
;
    }
    shaftRotating
    {
        type            kqRWallFunction;
        value           uniform 0.01;
    }
    shaft
    {
        type            kqRWallFunction;
        value           nonuniform 
0
;
    }
    sparger
    {
        type            kqRWallFunction;
        value           nonuniform 
0
;
    }
    stirrer
    {
        type            kqRWallFunction;
        value           uniform 0.01;
    }
    baffles
    {
        type            kqRWallFunction;
        value           nonuniform 
0
;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0.01;
    }
    AMI2
    {
        type            cyclicAMI;
        value           nonuniform 
0
;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.01;
    }
}


